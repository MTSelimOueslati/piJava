<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.chart.PieChart?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="534.0" prefWidth="796.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.FXMLpiechartController">
    <stylesheets>
        <URL value="@fxmlpiechart.css" />
    </stylesheets>
   <children>
      <PieChart fx:id="chart" layoutX="2.0" layoutY="14.0" prefHeight="472.0" prefWidth="387.0" title="Nombre des  produits par marque" />
      <BarChart fx:id="bchart" layoutX="390.0" prefHeight="508.0" prefWidth="379.0" title="prix par marque">
        <xAxis>
          <CategoryAxis side="BOTTOM" fx:id="x" />
        </xAxis>
        <yAxis>
          <NumberAxis fx:id="y" side="LEFT" />
        </yAxis>
      </BarChart>
      <Button layoutX="717.0" layoutY="495.0" mnemonicParsing="false" onAction="#retour" text="retour" />
   </children>
</AnchorPane>
